{
	"apiVersion": 2,
	"name": "rn/news-block",
	"title": "RN News Block",
	"category": "widgets",
	"icon": "smiley",
	"supports": {
		"html": false
	},
	"textdomain": "rn-news-block",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"attributes": {
		"topic": {
			"type": "string",
			"default": ""
		},
		"perPage": {
			"type": "number",
			"default": 10
		},
		"page": {
			"type": "number",
			"default": 1
		}
	}
}